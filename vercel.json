{
  "version": 2,
  "builds": [
    {
      "src": "api/search.js",
      "use": "@vercel/node"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "data/**",
      "use": "@vercel/static"
    },
    {
      "src": "**/*.*",
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/api/search",
      "destination": "/api/search.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
